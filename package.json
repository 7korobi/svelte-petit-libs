{
  "name": "svelte-petit-libs",
  "version": "0.0.4",
  "license": "MIT",
  "scripts": {
    "deploy": "run-s format build",
    "start": "sirv public",
    "build": "rollup -c",
    "check": "svelte-check",
    "css": "run-s make:sass make:brotli",
    "test": "run-s jest",
    "jest": "jest",
    "validate": "run-p format test check",
    "format": "prettier --write --no-semi --svelte-sort-order scripts-markup-styles ./src/",
    "make:sass": "sass --style compressed src/scss:public/css",
    "make:brotli": "find public -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k",
    "dev": "rollup -c -w"
  },
  "devDependencies": {
    "fake-indexeddb": "^3.1.2",
    "fastify": "^3.11.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "quicktype": "^15.0.258",
    "sirv-cli": "^1.0.10",
    "svelte-check": "^1.1.29",
    "svelte-jester": "^1.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript-json-schema": "^0.48.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "dexie": "^3.0.3",
    "fancy-date": "^0.4.1",
    "firebase": "^8.2.4",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "rollup": "^2.38.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "=6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "roosterjs": "^7.16.0",
    "sass": "^1.32.5",
    "svelte": "^3.32.0",
    "svelte-preprocess": "^4.6.3",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "ua-parser-js": "^0.7.23"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
